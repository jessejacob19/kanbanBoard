{
  "name": "eda_bolierplate",
  "version": "1.0.0",
  "description": "boilerplate for the stack taught at eda",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:server dev:client",
    "dev:server": "nodemon server",
    "dev:client": "npm run build -- --watch",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db-reset": "run-s db:*",
    "db:delete": "rm server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "build": "webpack --config webpack.config.js",
    "postinstall": "run-s db:migrate db:seed",
    "test": "jest --verbose tests/client/components tests/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessejacob19/eda_bolierplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "bugs": {
    "url": "https://github.com/jessejacob19/eda_bolierplate/issues"
  },
  "homepage": "https://github.com/jessejacob19/eda_bolierplate#readme",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.9.0",
    "express": "^4.17.0",
    "jest": "^24.8.0",
    "knex": "^0.17.0-next3",
    "nock": "^10.0.6",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.7",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sinon": "^7.3.2",
    "sqlite3": "^4.0.8",
    "superagent": "^5.0.5",
    "supertest": "^4.0.2",
    "thunk": "0.0.1",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
